{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    { "source": "/master-admin-dashboard", "destination": "/public/admin.html" },
    { "source": "/partner-panel", "destination": "/public/partner.html" },
    { "source": "/provider-panel", "destination": "/public/provider.html" },
    { "source": "/partner-registration", "destination": "/public/register.html" },
    { "source": "/(.*)", "destination": "/server.js" }
  ]
}
